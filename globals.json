        "IO": {
            "render": {
                "path": "{company}/render/{project}/{branch}/IO/{ingest_source}",
                "version": "{version}"
            },
            "source": {
                "path": "{company}/source/{project}/{branch}/IO/{ingest_source}",
                "version": "{version}"
            }
        },
        "assets": {
            "render": {
                "file": "/{filename}.{ext}",
                "path": "{company}/render/{project}/{branch}/assets/{episode}/{type}/{asset}/{task}/{variant}/{user}",
                "version": "{version}/{resolution}/{filename}.{ext}"
            },
            "source": {
                "path": "{company}/source/{project}/{branch}/assets/{episode}/{type}/{asset}/{task}/{variant}/{user}",
                "version": "{version}/{resolution}/{filename}.{ext}"
            }
        },
        "path_template": [
            "company",
            "context",
            "project",
            "branch",
            "scope"
        ],
        "shots": {
            "render": {
                "path": "{company}/render/{project}/{branch}/shots/{episode}/{seq}/{shot}/{task}/{variant}/{user}",
                "version": "{version}/{resolution}/{filename}.{ext}",
                "version_lite": "{version}/{resolution}/{render_pass}/{camera}/{aov}/{filename}.{ext}"
            },
            "source": {
                "path": "{company}/source/{project}/{branch}/shots/{episode}/{seq}/{shot}/{task}/{variant}/{user}",
                "version": "{version}/{resolution}/{filename}.{ext}"
            }
        },
        "unreal": {
            "assets": {
                "file": "{filename}.{ext}",
                "path": "content/assets/{type}/{asset}/{task}/{variant}"
            },
            "multiuser": {
                "port": 7000
            },
            "plugins": [
                "Water",
                "CodeView",
                "NiagaraExtras",
                "RigLogic",
                "LiveLink",
                "LiveLinkControlRig",
                "HairStrands"
            ],
            "project": {
                "path": "{company}/unreal/{unreal_project}/{filename}.{ext}"
            },
            "shots": {
                "file": "{filename}.{ext}",
                "path": "content/shots/{seq}/{shot}/{task}/{variant}"
            }
        }
    }
