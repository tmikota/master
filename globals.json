            }
        },
        "unreal": {
            "assets": {
                "file": "{filename}.{ext}",
                "path": "content/assets/{type}/{asset}/{task}/{variant}"
            },
            "multiuser": {
                "port": 7000
            },
            "plugins": [
                "Water",
                "CodeView",
                "NiagaraExtras",
                "RigLogic",
                "LiveLink",
                "LiveLinkControlRig",
                "HairStrands"
            ],
            "project": {
                "path": "{company}/unreal/{unreal_project}/{filename}.{ext}"
            },
            "shots": {
                "file": "{filename}.{ext}",
                "path": "content/shots/{seq}/{shot}/{task}/{variant}"
            }
        }
    }
